<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mainproject1"
    android:versionCode="1"
    android:versionName="1.0" >
    
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-sdk
        android:minSdkVersion = "14"
        android:targetSdkVersion = "16"/>

    <application
        android:icon="@drawable/yooz_48"
        android:label="@string/app_name"
        android:name = "MyApplication">
        <activity
            android:name=".Splash"
            android:label="@string/app_name" 
            android:configChanges = "orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name = "com.example.mainproject1.OTC" />
        <activity android:name = "com.example.mainproject1.AutoTopUp" />
        <activity android:name = "com.example.mainproject1.TopUp" />
        <activity android:name = "com.example.mainproject1.Accounts" />
        <activity android:name = "com.example.mainproject1.BillPayment" />
        <activity android:name = "com.example.mainproject1.P2P" />
        <activity android:name = "com.example.mainproject1.Paypal" />
        <activity android:name = "com.example.mainproject1.Register" />
        <activity android:name = "com.example.mainproject1.Wallet" />
        <activity android:name = "com.example.mainproject1.History" />
        <activity android:name = "com.example.mainproject1.EditAccount" />
        <activity android:name = "com.example.mainproject1.P2PRequest" />
        <activity android:name = "com.example.mainproject1.AddAccount" />
        <activity android:name="com.paypal.android.MEP.PayPalActivity"
			android:theme="@android:style/Theme.Translucent.NoTitleBar"
			android:configChanges="keyboardHidden|orientation"/>
        <activity android:name = "com.example.mainproject1.MainActivity" 
            android:configChanges = "orientation"/>
              
        
                       
    </application>

</manifest>